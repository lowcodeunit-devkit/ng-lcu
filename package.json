{
  "name": "@lowcodeunit-devkit/ng-lcu",
  "version": "0.0.183",
  "description": "A blank schematics",
  "scripts": {
    "deploy": "npm version patch && npm run build && npm run deploy:lib",
    "deploy:lib": "npm publish ./ --access public",
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "test:init": "rimraf test && mkdir test && cd test && ng new tester --createApplication=false --newProjectRoot=projects --directory=./",
    "test:ng-add": "schematics ../src/collection.json:ng-add --scope=@tester --workspace=tester"
  },
  "keywords": [
    "schematics"
  ],
  "author": "",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^7.3.1",
    "@angular-devkit/schematics": "^7.3.1",
    "@types/jasmine": "^3.0.0",
    "@types/node": "^8.0.31",
    "jasmine": "^3.0.0",
    "typescript": "~3.2.2"
  }
}
